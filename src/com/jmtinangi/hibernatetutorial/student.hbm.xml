<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jmtinangi.hibernatetutorial.Student" table="STUDENT">
		<id column="ID" name="id" type="long"></id>
		<property column="STUDENT_NAME" name="name" type="string"></property>
		<property column="DEGREE" name="degree" type="string"></property>
		<property column="ROLL" name="roll" type="string"></property>
		<property column="PHONE" name="phone" type="string"></property>
	</class>
</hibernate-mapping>